<p id="notice"><%= notice %></p>

<h1>Volatilities</h1>

<table>
  <thead>
    <tr>
      <th>Symbol</th>
      <th>Vol</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @volatilities.each do |volatility| %>
      <tr>
        <td><%= volatility.symbol %></td>
        <td><%= volatility.vol %></td>
        <td><%= volatility.date %></td>
        <td><%= link_to 'Show', volatility %></td>
        <td><%= link_to 'Edit', edit_volatility_path(volatility) %></td>
        <td><%= link_to 'Destroy', volatility, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Volatility', new_volatility_path %>
